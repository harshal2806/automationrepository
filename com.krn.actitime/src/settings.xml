
<!-- How to add Maven settings.xml file in Eclipse? If missing in workspace.
=======================================================================
copy code only and paste in notpad & save with file name "settings.xml". 
next, update <UserName> in below code with your system username.

Where can put maven "settings.xml" file location?
Windows machine: C:\Users\sam\.m2\settings.xml
On Mac OS X:  /home/sam/.m2/settings.xml 
======================================================================= -->

<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">3
          
  <!--  replace username with your system name   -->        
  <localRepository>Users/Sam/.m2/repository</localRepository>
 
  <interactiveMode>true</interactiveMode>
 
  <offline>false</offline>
 
  <pluginGroups>
    <!-- pluginGroup
     | Specifies a further group identifier to use for plugin lookup.
    <pluginGroup>com.your.plugins</pluginGroup>
    -->
  </pluginGroups>
 
